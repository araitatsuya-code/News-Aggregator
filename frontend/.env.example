# サイトの基本設定
NEXT_PUBLIC_SITE_URL=https://ai-news-aggregator.example.com
NEXT_PUBLIC_SITE_NAME=AI News Aggregator

# 分析・最適化設定
ANALYZE=false
NODE_ENV=production

# SEO設定
NEXT_PUBLIC_DEFAULT_LOCALE=ja
NEXT_PUBLIC_SUPPORTED_LOCALES=ja,en

# パフォーマンス設定
NEXT_PUBLIC_ENABLE_COMPRESSION=true
NEXT_PUBLIC_ENABLE_IMAGE_OPTIMIZATION=true

# 開発用設定
NEXT_PUBLIC_DEBUG_MODE=false